package A4_5_flujosDeControl;

public class A4_60_ForYArray {
	
	public static void main(String[] args) {
		
		
		String[]nombres = {"Andres","Pape","Maria","Paco","Luis","Bea","Pepa","asd"};
		
		//ESTA ES LA MANERA CORRECTA DE ITERAR SOBRE UN ARRAY, DECLARANDO EL CONTADOR FUERA DEL FOR, PARA QUE NO EVALÚE A CADA VUELTA DE BUCLE.
		int count = nombres.length;
		for(int i=0; i<count; i++) {
		
			System.out.println(nombres[i]);
		
		}	
	}
}
